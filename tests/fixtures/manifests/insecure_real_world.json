{
  "mcpServers": {
    "filesystem-overpermissive": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/", "/etc", "/var"],
      "description": "File system access for development"
    },
    "github-hardcoded": {
      "command": "npx", 
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
      }
    },
    "postgres-with-password": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-postgres", "postgresql://admin:supersecret123@db.example.com:5432/production"]
    },
    "slack-hardcoded": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-slack"],
      "env": {
        "SLACK_BOT_TOKEN": "xoxb-FAKE-TOKEN-REPLACE-ME",
        "SLACK_TEAM_ID": "T01234567"
      }
    },
    "sqlite-system-db": {
      "command": "uvx",
      "args": ["mcp-server-sqlite", "--db-path", "/var/lib/important-system.db"]
    },
    "puppeteer": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"]
    },
    "aws-hardcoded": {
      "command": "npx",
      "args": ["-y", "mcp-server-aws"],
      "env": {
        "AWS_ACCESS_KEY_ID": "AKIAIOSFODNN7EXAMPLE",
        "AWS_SECRET_ACCESS_KEY": "wJalrXUtnFEMI/K7MDENG/bPxRfiCYEXAMPLEKEY"
      }
    },
    "openai-key": {
      "command": "node",
      "args": ["custom-llm-server.js"],
      "env": {
        "OPENAI_API_KEY": "sk-proj-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
      }
    },
    "docker-privileged": {
      "command": "docker",
      "args": ["run", "--privileged", "-v", "/:/host", "mcp-server-image"]
    }
  }
}
